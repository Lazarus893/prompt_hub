[
    {
        "type": "function",
        "function": {
            "name": "forge",
            "parameters": {
                "type": "object",
                "required": [
                    "name",
                    "description",
                    "code",
                    "interval_in_second"
                ],
                "properties": {
                    "code": {
                        "type": "string",
                        "description": "runnable code of the strategy, without the codeblock, i.e. the '```jsx' part."
                    },
                    "name": {
                        "type": "string",
                        "description": "summary name of the strategy."
                    },
                    "visible": {
                        "type": "boolean",
                        "description": "whether the strategy is visible to other users, default false."
                    },
                    "config_json": {
                        "type": "string",
                        "description": "any other config of the strategy, default empty."
                    },
                    "description": {
                        "type": "string",
                        "description": "a playful and vivid description for the strategy, within 30 words."
                    },
                    "twitter_handle": {
                        "type": "string",
                        "description": "twitter handle of the user, default empty."
                    },
                    "interval_in_second": {
                        "type": "number",
                        "description": "a reasonable cron interval for the strategy, in seconds."
                    }
                }
            },
            "description": "Deploy the javascript code to a cronjob system to run periodically."
        }
    },
